	<li><p><%= f.label :double %>: <%= f.check_box :double %></p></li>
	<li><p><%= f.label :king %>: <%= f.check_box :king %></p></li>
	<li><p><%= f.label :murphy %>: <%= f.check_box :murphy %></p></li>
	<li><p><%= f.label :queen %>: <%= f.check_box :queen %></p></li>
	<li><p><%= f.label :rollaway %>: <%= f.check_box :rollaway %></p></li>
	<li><p><%= f.label :sofa %>: <%= f.check_box :sofa %></p></li>
	<li><p><%= f.label :twin %>: <%= f.check_box :twin %></p></li>
